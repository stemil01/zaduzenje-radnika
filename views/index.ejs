<!DOCTYPE html> 
 <html>
    <head>
        <%- include('partials/head') %>
    </head>
    <body>
        <h1>Djora Vet</h1>

        <p>Radnici:</p>
        <table>
            <% for (let i = 0; i < rows.length; i++) { %>
            <tr>
                <% for (let j = 0; j < rows[i].length; j++) { %> 
                    <th>
                        <%= rows[i][j] %>
                    </th>
                <% } %> 
            </tr>
            <% } %>
        </table>

        <button type="button" onclick="insert_Artikl()">Ubaci artikl</button>
        <a href="klik.ejs">Kliknite ovde</a>
        <script>
            let db = <%- db %>;
            function insert_Artikl() {
                db.run(`SELECT *
                        FROM Artikl`, (err) => {
                    if (err) {
                        console.log(err);
                    }
                })
            }
        </script>
    </body>
    <footer>
        <%- include('partials/footer') %>
    </footer>
</html>